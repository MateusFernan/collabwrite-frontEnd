{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "collabwrite": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/collabwrite",
      "sourceRoot": "projects/collabwrite/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/collabwrite",
            "index": "projects/collabwrite/src/index.html",
            "browser": "projects/collabwrite/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/collabwrite/tsconfig.app.json",
            "fileReplacements": [
              {
                "replace": "projects/collabwrite/src/environments/environment.ts",
                "with": "projects/collabwrite/src/environments/environment.prod.ts"
              }
            ],
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/collabwrite/public"
              },
              "projects/collabwrite/src/assets"
            ],
            "styles": ["projects/collabwrite/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "collabwrite:build:production",
              "fileReplacements": [
                {
                  "replace": "projects/collabwrite/src/environments/environment.ts",
                  "with": "projects/collabwrite/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "buildTarget": "collabwrite:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/collabwrite/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/collabwrite/public"
              },
              "projects/collabwrite/src/assets"
            ],
            "styles": ["projects/collabwrite/src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
